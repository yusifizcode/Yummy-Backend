@model List<Chef>
@{
    ViewData["Title"] = "Index";
}

<div class="container">
    <div class="row">
        <div class="col">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">FullName</th>
                        <th scope="col">Profession</th>
                        <th scope="col">Desc</th>
                        <th scope="col">Image</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <td>@item.FullName</td>
                        <td>@item.Profession</td>
                        <td>@item.Desc</td>
                        <td>@item.Image</td>
                    </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>

